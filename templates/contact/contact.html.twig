{% extends 'base.html.twig' %}

{% block title %} Nous-contacter {% endblock %}

{% block body %} 
    <h1>Nous-contacter</h1> <br>
    {{ form_start(form) }}
        <div class="row d-flex">
            <div class="col-4">{{ form_row(form.name) }}</div>
            <div class="col-4">{{ form_row(form.email) }}</div>
            <div class="col-4">{{ form_row(form.service) }}</div>
        </div>
        {{ form_rest(form) }}
    {{ form_end(form) }}
{% endblock %}